<p><a href="{{ path('gt.catalog.classificators.import_form') }}">Import</a></p>

{{ form(form) }}

<table>
{% for c in classificators %}
    <tr>
        <td>{{ c.code }}</td>
        <td>{{ c.group.name }}</td>
        <td>
            <a href="{{ path('gt.catalog.classificator_edit', {code: c.code}) }}" class="btn btn-primary">
                {% trans %}Edit{% endtrans %}
            </a>
        </td>
    </tr>
{% endfor %}
</table>
